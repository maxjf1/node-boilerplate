{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Node boilerplate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxjf1/node-boilerplate.git"
  },
  "license": "ISC",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "prettier": "prettier-check \"src/**/*.js\"",
    "database": "docker-compose up"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "main": "./dist/index.js",
  "devDependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/node": "*",
    "@babel/preset-env": "*",
    "eslint": "*",
    "nodemon": "*"
  }
}
